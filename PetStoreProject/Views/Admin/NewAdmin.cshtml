@model PetStoreProject.Models.Admin


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>NewAdmin</title>
</head>
<body>
    <form class="login" method="post" asp-action="AddNewAdmin">
        <div>
            <div class="login-table">
                <label asp-for="UserName">Add new UserName</label>
            </div>
            <input asp-for="UserName" />
            <span asp-validation-for="UserName"></span>
        </div>
        <div>
            <div class="login-table">
                <label asp-for="Password">Add new Password</label>
            </div>
            <input asp-for="Password" />
            <span asp-validation-for="Password"></span>
        </div>
        <br />
        <div>
            <input type="submit" value="Add" />
        </div>
    </form>
</body>
</html>
